@page "/"

@using Microsoft.AspNetCore.WebUtilities
@using myspace.Extensions

@inject NavigationManager NavManager

<h1>Välkommen</h1>

<p>Sidan är byggd på microsofts blazor och hostad på github. </p>

<!--
<a href="jpage.html" target="_blank">Jpage</a>
-->

@code {
    public string redirectURL = "";

    protected override void OnAfterRender(bool firstRender)
    {
        if (firstRender)
        {
            GetURL();
            if (!String.IsNullOrEmpty(redirectURL))
            {
                NavManager.NavigateTo(redirectURL);
            }
        }
    }

    void GetURL()
    {
        redirectURL = NavManager.QueryString("redirectURL");
    }
}



